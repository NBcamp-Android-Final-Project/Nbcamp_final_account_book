<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 배경 -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <!-- 위쪽 테두리 -->
    <item>
        <inset
            android:top="-1dp">
            <shape android:shape="rectangle">
                <stroke android:color="@color/cheon_primary_selected_home_icon" android:width="1dp"/>
            </shape>
        </inset>
    </item>
    <!-- 아래쪽 테두리 -->
    <item>
        <inset
            android:bottom="-1dp">
            <shape android:shape="rectangle">
                <stroke android:color="@color/cheon_primary_selected_home_icon" android:width="1dp"/>
            </shape>
        </inset>
    </item>
</layer-list>
